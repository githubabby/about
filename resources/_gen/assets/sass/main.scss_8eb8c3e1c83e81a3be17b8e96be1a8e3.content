*{margin:0;padding:0;box-sizing:border-box}body{font-family:Arial,sans-serif;background-color:#f7f5f2;color:#333;line-height:1.6}header{background-color:#fdf5e6;padding:20px;text-align:center}nav{background-color:#f5f0e6;border-bottom:2px solid #d1cfc4;padding:10px 20px;text-align:center;position:sticky;top:0;z-index:1000}nav ul{list-style:none;padding:0;margin:0;display:flex;justify-content:center;flex-wrap:wrap}nav ul li{margin:0 15px}nav ul li a{text-decoration:none;color:#333;font-weight:700}nav ul li a:hover{color:#6d6d6d}main{padding:20px;max-width:1200px;margin:0 auto}section{text-align:center;margin-bottom:20px}h2{color:#6d6d6d;border-bottom:2px solid #d1cfc4;padding-bottom:5px;margin-bottom:10px}ul{list-style:none;padding-left:0}ul li{padding:10px 0;border-bottom:1px solid #eee}blockquote{padding:10px;border-left:4px solid #d1cfc4;margin:10px 0;background-color:#f9f9f9}blockquote p{margin-bottom:5px}blockquote cite{display:block;text-align:right;color:#6d6d6d}footer{background-color:#f5f0e6;padding:10px;text-align:center;border-top:2px solid #d1cfc4;position:fixed;width:100%;bottom:0}footer p{color:#333}.socials a{color:#6d6d6d;text-decoration:none}.socials a:hover{text-decoration:underline}.skills{text-align:center}.skill-icons{display:flex;justify-content:center;flex-wrap:wrap;gap:20px;padding:20px}.skill-item{display:flex;flex-direction:column;align-items:center;justify-content:center;width:120px}.skill-item i{font-size:3rem;color:#333;margin-bottom:10px}.skill-item p{margin:0;font-size:1rem;color:#333}